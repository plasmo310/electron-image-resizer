{
    /* electron override settings */
    "extends": "./tsconfig.json",
    "compilerOptions": {
      /* commonjs */
      "target": "esnext",
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true,
      "esModuleInterop": true,
      "lib": ["esnext", "DOM", "DOM.Iterable"],
      "skipLibCheck": true,
      
      /* bundler mode */
      "allowImportingTsExtensions": false,
      "noEmit": false,

      "outDir": "dist/electron"
    },
    "include": ["src/electron/**/*"]
  }
  